{
    "name": "nest_serverless",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "npm run clean && NODE_OPTIONS='--max-old-space-size=4096' webpack --config webpack.config.js --mode=production",
        "build:dev": "webpack --config webpack.config.js --mode=development",
        "build:analyze": "npm run build && npm run analyze-bundle",
        "analyze-bundle": "npx webpack-bundle-analyzer dist/main.js --mode=static --report dist/bundle-report.html --no-open",
        "clean": "rm -rf dist .serverless node_modules/.cache",
        "lambda:deploy:dev": "ulimit -n 8192 && npm run build && serverless deploy --stage dev --aws-profile conta-stitcloud --region sa-east-1 --force",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.840.0",
        "@aws-sdk/lib-dynamodb": "^3.840.0",
        "@fastify/aws-lambda": "^5.1.4",
        "@fastify/static": "^6.10.1",
        "@nestjs/axios": "^3.0.1",
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.0.0",
        "@nestjs/platform-fastify": "^9.4.0",
        "@types/uuid": "^10.0.0",
        "aws-lambda": "^1.0.7",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.2",
        "serverless": "^3.34.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/aws-lambda": "^8.10.119",
        "@types/jest": "29.5.0",
        "@types/node": "18.15.11",
        "@types/supertest": "^2.0.11",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^16.0.0",
        "jest": "^30.0.0",
        "prettier": "^3.4.2",
        "serverless-esbuild": "^1.55.1",
        "serverless-offline": "^12.0.4",
        "serverless-plugin-optimize": "^4.2.1-rc.1",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^6.0.1"
    }
}
